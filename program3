#include <stdio.h>

int main() {
    int n, i, fact = 1;

    // Take input from user
    printf("Enter a number: ");
    scanf("%d", &n);

    // Calculate factorial
    for (i = 1; i <= n; i++) {
        fact *= i;
    }

    // Print factorial
    printf("Factorial of %d is %d\n", n, fact);

    return 0;
}
